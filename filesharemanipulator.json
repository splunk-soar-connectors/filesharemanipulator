{
    "appid": "ed94736c-672a-4f6c-b4c7-eb7bcc2465c0",
    "name": "File Share Manipulator",
    "description": "File share manipulator is an application that has the ability to manipulate files on a specific server",
    "type": "network device",
    "product_vendor": "Splunk",
    "logo": "logo_filesharemanipulator.svg",
    "logo_dark": "logo_filesharemanipulator_dark.svg",
    "product_name": "Splunk",
    "python_version": "3",
    "fips_compliant": false,
    "product_version_regex": ".*",
    "publisher": "Splunk Community",
    "contributors": [
        {
            "name": "Patryk Dros"
        }
    ],
    "license": "Copyright (c) 2023 Splunk Inc.",
    "app_version": "1.0.1",
    "utctime_updated": "2023-01-12T17:16:09.279151Z",
    "package_name": "phantom_filesharemanipulator",
    "main_module": "filesharemanipulator_connector.py",
    "min_phantom_version": "5.5.0",
    "app_wizard_version": "1.0.0",
    "configuration": {
        "username": {
            "data_type": "string",
            "description": "Username",
            "order": 0,
            "required": true
        },
        "password": {
            "data_type": "password",
            "description": "Password",
            "order": 1,
            "required": true
        },
        "ip_address": {
            "data_type": "string",
            "description": "IP address of server with which we want to connect",
            "order": 2,
            "required": true
        },
        "domain": {
            "data_type": "string",
            "description": "Domain of server with which we want to connect",
            "order": 3
        }
    },
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Validate the asset configuration for connectivity using supplied configuration",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "get file",
            "identifier": "get_file",
            "description": "Get file from the Network share, such action will return vault id number of file",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "share_name": {
                    "description": "Share name value",
                    "data_type": "string",
                    "required": true,
                    "order": 0
                },
                "file_path": {
                    "description": "Whole path to the file which we want to download from the server",
                    "data_type": "string",
                    "required": true,
                    "order": 1
                }
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "column_name": "Status",
                    "column_order": 0,
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.parameter.file_path",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.parameter.share_name",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.data.*.vault_id",
                    "data_type": "string",
                    "contains": [
                        "vault id"
                    ],
                    "column_name": "vault_id",
                    "column_order": 1
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric"
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric"
                }
            ],
            "versions": "EQ(*)"
        },
        {
            "action": "put file",
            "identifier": "put_file",
            "description": "Put file to the Network share",
            "type": "generic",
            "read_only": false,
            "parameters": {
                "share_name": {
                    "description": "Share name value",
                    "data_type": "string",
                    "required": true,
                    "order": 0
                },
                "path": {
                    "description": "Whole path to place where you want to have a file",
                    "data_type": "string",
                    "default": "/",
                    "order": 1
                },
                "vault_id": {
                    "description": "Vault ID of file which you want to put from Container (Event)",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "vault id"
                    ],
                    "order": 2
                }
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "column_name": "Status",
                    "column_order": 1,
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.parameter.share_name",
                    "data_type": "string",
                    "column_name": "File Name",
                    "column_order": 0
                },
                {
                    "data_path": "action_result.parameter.path",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.parameter.vault_id",
                    "data_type": "string",
                    "contains": [
                        "vault id"
                    ],
                    "column_name": "Vault ID",
                    "column_order": 2
                },
                {
                    "data_path": "action_result.data",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric"
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric"
                }
            ],
            "versions": "EQ(*)"
        }
    ],
